includeScript "Scripts/computerattack"includeScript "Levels/AntackFight1/HUDInit"includeScript "Scripts/missileattach"def "quit"def "qtime"//means quit time, not QuickTimeseti 1,0storei "quit",1def "p1_maxx"def "p1_minx"setf 3,77.1setf 4,38.35storef "p1_maxx",3storef "p1_minx",4///Turn on mouse viewsetb 1,truestoreb "doMouseView",1storeb "doFog",1setf 1,400.0storef "gFogEnd",1//Do random RGB:randi 5,15,1setf 1,i1multf 1,0.1setf 2,f1setf 3,f1rgb//loopplayersetb 1,trueseti 2,0loadi "quit",1if i,1,>,2get "block1"storeb "do",1time 2loadf "qtime",1if f,2,>=,1seti 3,1if i,1,=,3//level "Levels/AntackGUI1"level "Levels/AntackFight2"elselevel "Levels/AntackGUI2"endifendifendifrandi 1,40,3seti 2,40if i,3,=,2randi -100,100,1randi -100,100,2randi -100,100,3setf 1,i1setf 2,i2setf 3,i3randi 19,20,1sndplayendifclick 2,nif b,2,=,1mousesetb 3,truesets 1,"shot1"getloadb "do",2if b,2,=,1sets 1,"shot2"getloadb "do",2if b,2,=,1sets 1,"shot3"getloadb "do",2if b,2,=,1setb 3,falseendifendifendifif b,3,=,1putstoref "x",4storef "y",5storef "z",6storeb "do",1time 7storef "time",7seti 1,1sndplaysetb 2,falsestoreb "t",2getTeamteampoke 2,7seti 1,2if i,2,=,1playergetTeam 4get collgetTeam 3if i,4,!=,3mark 4getName 2getstores "target",2storeb "t",1endifendifendifendifkey "w",2key "s",3key "a",4key "d",5key "r",6key "f",7key 53,8if b,8,=,1level "Levels/AntackGUI0"endifif b,6,=,1markpassed 4multf 4,12.25addf 2,f4putendifif b,7,=,1markpassed 4multf 4,-12.25addf 2,f4putendifif b,2,=,1passed 1multf 1,12.25moveendifif b,3,=,1passed 1multf 1,-12.25moveendifif b,4,=,1axisaddf 2,90spinpassed 1multf 1,12.25moveaxisaddf 2,-90spinendifif b,5,=,1axisaddf 2,-90spinpassed 1multf 1,12.25moveaxisaddf 2,90spinendifseti 1,0getTeam 2if i,1,=,2///handle keyboard input of player 0markloadf "p1_maxx",4if f,1,>,4setf 1,f4//putendifloadf "p1_minx",4if f,1,<,4setf 1,f4//putendifkey "1",2,nif b,2,=,1mac "p01"endifkey "2",2,nif b,2,=,1#mac "p02"endifkey "3",2,nif b,2,=,1#mac "p03"endifkey "4",2,nif b,2,=,1#mac "p04"endifkey "5",2,nif b,2,=,1#mac "p05"endifkey "6",2,nif b,2,=,1#mac "p06"endif//else//player 1 keyboard inputsetb 1,truekey "1",2,nif b,2,=,1#mac "p11"endifkey "2",2,nif b,2,=,1#mac "p12"endifkey "3",2,nif b,2,=,1#mac "p13"endifkey "4",2,nif b,2,=,1#mac "p14"endifkey "5",2,nif b,2,=,1#mac "p15"endifkey "6",2,nif b,2,=,1#mac "p16"endifendifendloop