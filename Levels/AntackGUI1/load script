setb 1,truestoreb "doMouseView",1storeb "doFog",1get "background1"hud truelux trueget "block1"hud truelux truealpha 1,1def "time",time 1storef "time",1channel 2playermarkseti 1,7sndplaychannel -1loopsetb 1,truetime 2loadf "time",1multf 1,-1addf 2,f1setf 3,55.0if f,2,>=,3level "Levels/AntackGUI0"endifclick 2if b,2,=,1sndstop 2level "Levels/AntackGUI0"endifendloop