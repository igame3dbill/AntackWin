def "ct"//computer timetime 1storef "ct",1loopplayergetTeamseti 2,0if i,2,=,1//player is in team 0, now we handle player 1 as computer!time 4loadf "ct",3multf 3,-1addf 4,f3setf 2,3.0if f,4,>=,2time 4storef "ct",4randi 1,3,1repeatrandi 1,6,1seti 3,0seti 2,1if i,2,=,1get "player01"loadi "life",4if i,4,<=,3addi 1,1elsemac "p11"endifendifseti 2,2if i,2,=,1get "player02"loadi "life",4if i,4,<=,3addi 1,1elsemac "p12"endifendifseti 2,3if i,2,=,1get "player03"loadi "life",4if i,4,<=,3addi 1,1elsemac "p13"endifendifseti 2,4if i,2,=,1get "player04"loadi "life",4if i,4,<=,3addi 1,1elsemac "p14"endifendifseti 2,5if i,2,=,1get "player05"loadi "life",4if i,4,<=,3addi 1,1elsemac "p15"endifendifseti 2,6if i,2,=,1get "player06"loadi "life",4if i,4,<=,3mac "p11"elsemac "p16"endifendifendrepeatendifendifendloop