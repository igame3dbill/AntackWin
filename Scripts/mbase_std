///For all missile basesget medef "busy",pdef "targetX",pdef "targetY",pdef "targetZ",pdef "life",pdef "dead",pdef "mis",pdef "t",psetb 1,falsestoreb "busy",1storeb "dead",1seti 1,5storei "life",1setb 1,truestoreb "mis",1loopsetb 1,trueget meloadb "dead",2if b,2,!=,1seti 1,0seti 3,5loadi "life",2if i,2,<=,1storeb "busy",1storeb "dead",1markaddf 2,-200.0putmac "stat"get meloadi "snd",1playermarksndplayelseif i,2,!=,3setf 3,10.0loadf "t",1time 2multf 1,-1.0addf 2,f1if f,2,>=,3addi 2,1storei "life",2time 2storef "t",2setf 1,i2multf 1,50.0sizeendifendifendifelsestoreb "busy",1endifendloop